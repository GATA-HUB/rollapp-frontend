{
  "name": "gata-rollapp",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.12.0",
  "scripts": {
    "dev": "pnpm --filter @gata-rollapp/web dev",
    "build": "pnpm --filter @gata-rollapp/web build",
    "start": "pnpm --filter @gata-rollapp/web start",
    "contracts:compile": "pnpm --filter @gata-rollapp/contracts compile",
    "contracts:test": "pnpm --filter @gata-rollapp/contracts test",
    "contracts:deploy": "pnpm --filter @gata-rollapp/contracts deploy",
    "clean": "pnpm -r run clean",
    "build:all": "pnpm -r run build"
  }
}
